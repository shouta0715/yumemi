{
  "name": "yumemi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true pnpm build",
    "lint": "next lint",
    "format": "prettier --write . && eslint --fix .",
    "format:check": "prettier --check . && eslint .",
    "type-check": "tsc --noEmit",
    "test": "dotenv -e .env.test vitest -c ./vitest.config.mts",
    "test:ui": "dotenv -e .env.test -- vitest -c ./vitest.config.mts --ui",
    "test:coverage": "dotenv -e .env.test -- vitest -c ./vitest.config.mts --coverage",
    "test:e2e": "dotenv -e .env.test playwright test",
    "test:e2e:ui": "dotenv -e .env.test playwright test --ui",
    "test:e2e:debug": "dotenv -e .env.test playwright test --debug",
    "test:e2e:report": "dotenv -e .env.test npx playwright show-report",
    "prepare": "husky",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.1.4",
    "clsx": "^2.1.0",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.25",
    "@playwright/test": "^1.42.1",
    "@storybook/addon-a11y": "^8.0.5",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-onboarding": "^8.0.5",
    "@storybook/blocks": "^8.0.5",
    "@storybook/nextjs": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "happy-dom": "^14.3.9",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "storybook": "^8.0.5",
    "typescript": "^5",
    "vitest": "^1.4.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  }
}
